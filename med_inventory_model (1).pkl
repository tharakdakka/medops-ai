   "source": [
    "import pandas as pd\n",
    "from sklearn.ensemble import RandomForestRegressor\n",
    "import joblib\n",
    "import numpy as np\n",
    "\n",
    "# 1. Create simple training data\n",
    "# We use 'stock' and 'flu_rate' to predict 'demand'\n",
    "data = {\n",
    "    'stock': [100, 200, 150, 300, 50, 400],\n",
    "    'day': [0, 0, 0, 0, 0, 0], # Placeholder for the day\n",
    "    'flu_rate': [5, 10, 7, 15, 2, 20],\n",
    "    'demand': [20, 45, 30, 60, 10, 85]\n",
    "}\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# 2. Define Features (X) and Target (y)\n",
    "X = df[['stock', 'day', 'flu_rate']]\n",
    "y = df['demand']\n",
    "\n",
    "# 3. Train the model\n",
    "model = RandomForestRegressor(n_estimators=100)\n",
    "model.fit(X, y)\n",
    "\n",
    "# 4. Save the model as a .pkl file\n",
    "joblib.dump(model, \"med_inventory_model.pkl\")\n",
    "print(\"Success! med_inventory_model.pkl has been created.\")"
   ]
